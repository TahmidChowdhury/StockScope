{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.backend"
  },
  "deploy": {
    "startCommand": "python main.py",
    "healthcheckPath": "/health"
  },
  "environments": {
    "production": {
      "variables": {
        "PYTHONPATH": "/app"
      }
    }
  }
}